<h2> Season <%=@bach.season_number%> Contestants </h2>

<% @contestants.each do |contestant| %>
  <ul><%= link_to contestant.name, contestant_path(contestant) %>
  <li>Age: <%=contestant.age %>
  <li>Hometown: <%=contestant.hometown %>
<% end %> 

<section id="Hometowns">
  <h3>These Contestants are from these hometowns: </h3>
  <%= @bach.hometown_list %>
</section>